= get_facebook_script_tag

.container
  - @feeds.each do |feed|
    %h4= link_to feed.site_name, feed.site_url, class: 'site-title', target: '_blank'

    = check_entries(feed)

    %table.table.table-striped
      - feed.entries.each do |entry|
        %tr
          %td
            %small
              = create_publish_date(entry.publish_datetime.to_s)
            %br
            .entry-link-container
              = link_to entry.title, entry.url, target: '_blank'
              = show_social_buttons(entry)

= get_google_plus_script_tag